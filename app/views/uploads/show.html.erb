<br>
<div class="row">
  <p class="col-md-8"><b>Name: </b><%= @upload.name %></p>
  <p class="col-md-3"><b>Size: </b><%= @upload.file_size %></p>
</div>
<div >
  <%= link_to "Download File", @upload.file_url %> |
  <%= link_to 'Edit', edit_upload_path(@upload) %> |
  <%= link_to 'Destroy', @upload, data: { confirm: 'Are you sure?' }, method: :delete %> |
  <%= link_to 'Back', uploads_path %>
</div>